{
  "name": "bearon-ui-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/bearon-ui-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/bearon-ui-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "bearon-ui:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "bearon-ui:serve:production"
        },
        "ci": {
          "devServerTarget": "bearon-ui:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/bearon-ui-e2e/**/*.{js,ts}"]
      }
    }
  },
  "implicitDependencies": ["bearon-ui"],
  "tags": []
}
