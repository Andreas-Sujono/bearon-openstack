{
  "extends": ["plugin:@nx/react", "../../.eslintrc.base.json"],
  "ignorePatterns": ["!**/*"],
  "plugins": ["react-refresh"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ],
  "rules": {
    "react-refresh/only-export-components": "warn"
  }
}
